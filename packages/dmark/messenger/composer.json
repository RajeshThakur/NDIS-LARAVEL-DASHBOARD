{
    "name": "dmark/messenger",
    "description": "Messaging module for NDIS",
    "keywords": ["laravel", "messenger", "messaging", "user messaging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rajesh Thakur",
            "email": "rajesh.thakur@dmarkweb.com",
            "homepage": "https://github.com/dmarkRajesh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": ">=5.5 <6.0",
        "illuminate/support": ">=5.5 <6.0",
        "illuminate/database": ">=5.5 <6.0"
    },
    "require-dev": {
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.5",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Dmark\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmark\\Messenger\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmark\\Messenger\\MessengerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
