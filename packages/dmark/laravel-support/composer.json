{
    "name": "dmark/laravel-support",
    "description": "Dmark common support helpers, contracts, and traits required by various Dmark packages. Validator functionality, and basic controller included out-of-the-box.",
    "type": "library",
    "keywords": [
        "dmark",
        "laravel",
        "contract",
        "validator",
        "timezones",
        "mimetype",
        "support",
        "helper",
        "trait"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.0",
        "illuminate/auth": "~5.8.0",
        "illuminate/bus": "~5.8.0",
        "illuminate/contracts": "~5.8.0",
        "illuminate/http": "~5.8.0",
        "illuminate/routing": "~5.8.0",
        "illuminate/support": "~5.8.0",
        "illuminate/validation": "~5.8.0",
        "spatie/laravel-schemaless-attributes": "^1.4.0",
        "watson/validating": "^3.2.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.1.0"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Dmark\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmark\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
